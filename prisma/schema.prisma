// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    // url      = "file:./db.sqlite"
    url      = env("DATABASE_URL")
}

model file {
    id            String  @id
    name          String
    password      String?
    fileUrl       String
    type          String
    path          String
    downloadCount Int     @default(0)
}
